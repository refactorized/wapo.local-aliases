alias dc="docker-compose"
alias dc-nuke-rebuild="dc build --force-rm --no-cache --pull"
alias undocker='docker stop $(docker ps -a -q); docker rm $(docker ps -a -q)'
alias unpm="find . -name "node_modules" -type d -prune -exec rm -rf '{}' +"
alias wpvpn="sudo openconnect --user `whoami` --juniper https://ra.washpost.com --authgroup=TWP-contractor"
